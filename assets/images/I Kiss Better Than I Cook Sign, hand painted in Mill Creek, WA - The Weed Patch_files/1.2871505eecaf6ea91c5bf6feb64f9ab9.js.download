      var _type = 'POST';
      var _data = postData;
    }

    return ajax.query.add(
      {
        type: _type,
        url: src,
        data: _data,
        success: function(a, b, c, d) {
          return o.callback(true, a, b, c, d);
        },
        error: function(a, b, c, d) {
          this.close();
          return o.callback(false, a, b, c, d);
